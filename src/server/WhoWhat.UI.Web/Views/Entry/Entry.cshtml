                            pager: true,
                            nav: false,
                            speed: 500,
                            namespace: "callbacks",
                            before: function () {
                                $('.events').append("<li>before event fired.</li>");
                            },
                            after: function () {
                                $('.events').append("<li>after event fired.</li>");
                            }
                        });

                    });
                </script>
                <!--//End-slider-script -->
                <div id="top" class="callbacks_container">
                    <ul class="rslides" id="slider4">
                        <li>
                            <div class="head-info">
                                <span>
                                    <img src="/Content/images/ask.frame.png" alt="whowhat ask" />
                                </span>
                                <div class="head-text">
                                    <h1>Ask a question and get answers</h1>
                                    <p>
                                        Choose a photo or make it, write a question and add related tags. Keep your question brief.
                                        Use Twitter style. No question can be asked without a photo.
                                    </p>
                                    <div class="button">
                                        <a href="http://www.windowsphone.com/en-us/store/app/who-what/25dd4500-c7f4-4d02-a5f1-ca86cacc442d">DOWNLOAD</a>
                                    </div>
                                </div>
                                <div class="mobile-device">
                                    <img src="/Content/images/ask.frame.png" alt="whowhat ask" />
                                </div>
                                <div class="clearfix"></div>
                            </div>
                        </li>
                        <li>
                            <div class="head-info">
                                <span>
                                    <img src="/Content/images/feed.frame.png" alt="whowhat feed" />
                                </span>
                                <div class="head-text">
                                    <h1>Browse differend feeds</h1>
                                    <p>Both anonymous and registered users can view popular, live and unanswered questions and search them.</p>
                                    <div class="button">
                                        <a href="http://www.windowsphone.com/en-us/store/app/who-what/25dd4500-c7f4-4d02-a5f1-ca86cacc442d">DOWNLOAD</a>
                                    </div>
                                </div>
                                <div class="mobile-device">
                                    <img src="/Content/images/feed.frame.png" alt="whowhat feed" />
                                </div>
                                <div class="clearfix"></div>
                            </div>
                        </li>
                        <li>
                            <div class="head-info">
                                <span>
                                    <img src="/Content/images/profile.frame.png" alt="whowhat profile" />
                                </span>
                                <div class="head-text">
                                    <h1>View user profiles</h1>
                                    <p>You can create and view your own user profile as well as browse through the profiles of other users. </p>
                                    <div class="button">
                                        <a href="http://www.windowsphone.com/en-us/store/app/who-what/25dd4500-c7f4-4d02-a5f1-ca86cacc442d">DOWNLOAD</a>
                                    </div>
                                </div>
                                <div class="mobile-device">
                                    <img src="/Content/images/profile.frame.png" alt="whowhat profile" />
                                </div>
                                <div class="clearfix"></div>
                            </div>
                        </li>
                        <li>
                            <div class="head-info">
                                <span>
                                    <img src="/Content/images/tag.search.frame.png" alt="whowhat search" />
                                </span>
                                <div class="head-text">
                                    <h1>Search for specific tags</h1>
                                    <p>The application provides the search functionality. Also just tap on a tag to display all questions tagged with it.</p>
                                    <div class="button">
                                        <a href="http://www.windowsphone.com/en-us/store/app/who-what/25dd4500-c7f4-4d02-a5f1-ca86cacc442d">DOWNLOAD</a>
                                    </div>
                                </div>
                                <div class="mobile-device">
                                    <img src="/Content/images/tag.search.frame.png" alt="whowhat search" />
                                </div>
                                <div class="clearfix"></div>
                            </div>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                    <!-- banner Slider Ends Here -->
                </div>
                <div class="clearfix"></div>
            </div>
            <!-- //container -->
        </div>
    </div>
    <!-- header -->
    <!-- welcome-note -->
    <div class="welcome-note">
        <h2>Welcome To Who What</h2>
        <p>
            Ask question and get answers!
            Choose a photo or make it, write a question and add related tags.
            Vote for noteworthy questions and answers.
            Your rating is increasing as other users vote for your questions or answers.
        </p>
    </div>
