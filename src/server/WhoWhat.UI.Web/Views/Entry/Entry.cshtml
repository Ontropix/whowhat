    <script type="text/javascript">
        $(document).ready(function () {
            $().UItoTop({ easingType: 'easeOutQuart' });

            $('#main-contact-form').submit(function () {
                var that = $(this);

                $.post(that.attr('action'), that.serialize(), function (data) {
                    debugger;

                    that.prev().text(data.Message).fadeIn().delay(3000).fadeOut();

                }, 'json');

                that.trigger('reset');

                return false;
            });

        });
    </script>
